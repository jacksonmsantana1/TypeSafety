{
  "name": "typesafety",
  "description": "Functional type checker",
  "main": "./dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha --compilers js:babel-core/register ./src/*.js --reporter nyan",
    "build": "babel ./src -o ./index.js --ignore *.spec.js -s",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacksonmsantana1/TypeSafety.git"
  },
  "author": "Jackson Martorano Sant'Ana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacksonmsantana1/TypeSafety/issues"
  },
  "homepage": "https://github.com/jacksonmsantana1/TypeSafety#readme",
  "dependencies": {
    "data.either": "^1.4.0",
    "ramda": "^0.21.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "commitizen": "^2.8.2",
    "cz-conventional-changelog": "^1.1.6",
    "mocha": "^2.5.3",
    "nyc": "^7.0.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitzen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
